{
  "name": "zendframework/zend-expressive-skeleton",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/cudev/ordinary-mail"
    }
  ],
  "require": {
    "php": "^7.0",
    "roave/security-advisories": "dev-master",
    "zendframework/zend-expressive": "^1.0",
    "zendframework/zend-expressive-helpers": "^2.0",
    "zendframework/zend-stdlib": "^3.0",
    "zendframework/zend-expressive-fastroute": "^1.1",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-expressive-twigrenderer": "^1.1",
    "ocramius/proxy-manager": "^2.0",
    "doctrine/orm": "2.5.*",
    "robmorgan/phinx": "^0.5.2",
    "guzzlehttp/guzzle": "^6.1",
    "predis/predis": "^1.0",
    "aura/session": "^2.0",
    "respect/validation": "^1.0",
    "league/flysystem": "^1.0",
    "cocur/slugify": "^2.0",
    "symfony/asset": "^3.0",
    "nesbot/carbon": "^1.21",
    "vlucas/phpdotenv": "^2.2",
    "aura/intl": "^1.1",
    "symfony/serializer": "^3.0",
    "tedivm/stash": "^0.14.1",
    "mobiledetect/mobiledetectlib": "^2.8",
    "phpmailer/phpmailer": "^5.2",
    "facebook/php-sdk-v4": "^5.1",
    "google/apiclient": "^2.0.0@RC",
    "abraham/twitteroauth": "^0.6.3",
    "tijsverkoyen/css-to-inline-styles": "^1.5",
    "hashids/hashids": "^1.0",
    "intervention/image": "^2.3",
    "symfony/property-access": "^3.0",
    "cudev/ordinary-mail": "dev-master",
    "cudev/game-feed": "^0.0.2@alpha"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.5",
    "filp/whoops": "^2.0",
    "symfony/var-dumper": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Ludos\\": "src/"
    }
  },
  "scripts": {
    "check": [
      "@cs",
      "@test"
    ],
    "cs": "phpcs",
    "cs-fix": "phpcbf",
    "serve": "php -S 0.0.0.0:8080 -t public/",
    "test": "phpunit"
  }
}
